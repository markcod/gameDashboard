@model IEnumerable<GameDashboard.Models.Spil>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Games</h1>

<div class="main-content">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(m => m.Filnavn)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Spil1)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Score)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelitem => item.Filnavn)
                    </td>
                    <td>
                        @Html.DisplayFor(modelitem => item.Spil1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelitem => item.Score)
                    </td>
                </tr>
            }
        </tbody>
    </table>

</div>